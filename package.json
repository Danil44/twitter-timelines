{
  "name": "twitter-timeline",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "v10.15.3"
  },
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm start --prefix client",
    "debug": "nodemon --inspect ./bin/www",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "http-errors": "^1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "oauth": "^0.9.15",
    "redux-logger": "^3.0.6",
    "request": "^2.88.0",
    "twitter": "^1.7.1"
  }
}