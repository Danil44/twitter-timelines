{
  "name": "twitter-timeline",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "v10.15.3",
    "yarn": "1.x"
  },
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "test": "mocha || true",
    "debug": "nodemon --inspect ./bin/www",
    "client": "cd client && yarn start",
    "client-tests": "cd client && yarn test",
    "client-install": "yarn install --prefix client",
    "install": "concurrently \"yarn install\" \"yarn run client-install\"",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "all-tests": "concurrently \"yarn test\" \"yarn run client-tests\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && yarn install && yarn run build --prefix client"
  },
  "dependencies": {
    "concurrently": "^4.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "http-errors": "^1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "oauth": "^0.9.15",
    "redux-logger": "^3.0.6",
    "request": "^2.88.0",
    "twitter": "^1.7.1",
    "yarn": "^1.16.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.1.4"
  }
}